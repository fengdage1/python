import re
#findall就是所有匹配到的集合，数据量太大该方法不适合，应该用findone遍历
html = html[0:npos]
reg = '(?<=src=\")[^\<\>]+(?=\")'
s = re.compile(reg)
//取所有
lst = s.findall(html) //lst是个list列表
if len(lst) == 0:
  return False
//取一个
r = s.search(html)
if r == None:
  return False
item = r.group()

#看是否找到，group()默认是group(0)
m = re.match(reg, url)
if m != None:
return m.group()
